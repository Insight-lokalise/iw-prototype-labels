{
  "name": "iw-checkout",
  "version": "07.14.2018",
  "description": "insightweb checkout implementation",
  "main": "index.js",
  "scripts": {
    "test": "buildkit-asserter --coverage",
    "test:watch": "buildkit-asserter --watch",
    "start": "buildkit-legacy-scripts start --use-app",
    "build": "buildkit-legacy-scripts build --use-app",
    "import-labels": "buildkit-legacy-scripts labels app-checkout --use-app",
    "profile": "cross-env NODE_ENV=production webpack --profile --json > bundleStats.json && npm run bundleReport",
    "bundleReport": "webpack-bundle-analyzer bundleStats.json -m static -r bundleReport.html dist/jcr_root/checkout",
    "lint": "npm run lint:js -- --fix",
    "lint:js": "eslint --quiet \"libs/**/*.js\" \"app/**/*.js\"",
    "prettify": "prettier --write --single-quote --tab-width 4 --trailing-comma all --no-semi \"libs/**/*.js\" \"app/**/*.js\"",
    "hardLint:js": "find src/ -name '*.js' ! -type d -exec bash -c $'expand -t 4 \"$0\" > /tmp/e && mv /tmp/e \"$0\"' {} \\;",
    "init-cq": "tinyreq -u http://admin:admin@localhost:4502/etc/clientlibs/insight-ui/iw-checkout/ -d jcr:primaryType=nt:folder"
  },
  "author": "Insight Enterprises, Inc",
  "license": "ISC",
  "devDependencies": {
    "buildkit-legacy-scripts": "^5.0.0-Beta.10",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.0.0",
    "axios-mock-adapter": "^1.19.0",
    "buildkit-asserter": "^1.0.36",
    "cross-env": "~4.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "prop-types": "^15.5.8",
    "redux-mock-store": "^1.5.4",
    "tinyreq-cli": "^1.0.6",
    "webpack": "^4.36.0",
    "webpack-bundle-analyzer": "~2.7.0",
    "webpack-merge": "^5.0.8",
    "webpack-shell-plugin": "~0.5.0"
  },
  "dependencies": {
    "@insight/toolkit-css-core": "^2.2.17",
    "@insight/toolkit-css-ui": "^2.2.182",
    "@insight/toolkit-react": "^2.2.183",
    "@insight/toolkit-utils": "^2.2.181",
    "axios": "^0.27.2",
    "card-validator": "~4.1.0",
    "classnames": "~2.2.5",
    "core-js": "~2.4.1",
    "credit-card-type": "~5.0.2",
    "date-fns": "2.16.1",
    "foundation-sites": "6.3.1",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "moment": "~2.29.4",
    "numeral": "~2.0.4",
    "raw-loader": "^4.0.1",
    "react": "~16.13.1",
    "react-autosize-textarea": "~0.4.3",
    "react-dom": "~16.13.1",
    "react-helmet": "^5.1.3",
    "react-overlays": "^4.1.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^4.1.1",
    "react-select": "~1.0.0-rc.4",
    "react-slick": "~0.16.0",
    "react-tabs": "^1.0.0",
    "redux": "~4.0.5",
    "redux-form": "^8.3.6",
    "redux-promise-middleware": "~6.1.2",
    "redux-thunk": "~2.3.0",
    "reselect": "~3.0.0",
    "whatwg-fetch": "~2.0.2"
  },
  "jestSonar": {
    "sonar56x": true,
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "false": {}
}
