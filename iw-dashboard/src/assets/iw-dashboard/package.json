    "@insight/toolkit-css-core": "^1.4.582",
    "@insight/toolkit-css-ui": "^1.7.27",
    "@insight/toolkit-react": "^2.2.15",
    "@insight/toolkit-utils": "^1.7.21",
    "axios": "^0.27.2",
    "classnames": "^2.2.5",
    "core-js": "^2.5.3",
    "iw-styles": "^0.2.12",
    "lodash-es": "^4.17.15",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.18.1",
    "powerbi-client": "^2.10.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-grid-layout": "^0.15.3",
    "react-html-parser": "^2.0.2",
    "react-measure": "^2.0.2",
    "react-overlays": "~0.8.0",
    "react-redux": "^7.2.1",
    "react-select": "^1.0.0-rc.5",
    "react-transition-group": "^1.2.1",
    "recharts": "^2.1.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.2.0"
  },
  "publishConfig": {
    "registry": "https://npm-repo.insight.com/repository/npm-nsit/"
  },
  "scripts": {
    "start": "buildkit-legacy-scripts start",
    "build": "buildkit-legacy-scripts build",
    "test": "buildkit-asserter --coverage",
    "aem-import": "buildkit-legacy-scripts aem-import",
    "import-labels": "buildkit-legacy-scripts labels app-dashboard",
    "test:watch": "buildkit-asserter --watch",
    "precommit": "buildkit-legacy-scripts lint"
