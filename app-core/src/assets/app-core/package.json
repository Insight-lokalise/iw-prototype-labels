{
  "name": "app-core",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "buildkit-legacy-scripts start",
    "build": "buildkit-legacy-scripts build",
    "aem-import": "buildkit-legacy-scripts aem-import",
    "test": "buildkit-asserter --coverage",
    "test:watch": "buildkit-asserter --watch",
    "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions"
  },
  "resolutions": {
    "faye": "1.1.3"
  },
  "jestSonar": {
    "sonar56x": true,
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "dependencies": {
    "@insight/toolkit-css-core": "^2.2.17",
    "@insight/toolkit-css-ui": "^2.2.195",
    "@insight/toolkit-react": "^2.2.195",
    "@insight/toolkit-utils": "^2.2.191",
    "axios": "^0.27.2",
    "date-fns": "2.16.1",
    "launchdarkly-js-client-sdk": "^2.20.1",
    "logrocket": "^3.0.1",
    "react": "^16",
    "react-dom": "^16"
  },
  "devDependencies": {
    "buildkit-legacy-scripts": "^5.0.0-Beta.8",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "buildkit-asserter": "^1.0.36"
  }
}
