  "license": "ISC",
  "devDependencies": {
    "buildkit-legacy-scripts": "^5.0.0-Beta.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "buildkit-asserter": "^1.0.36",
    "owc-dev-server": "^1.0.0"
  },
  "dependencies": {
    "lit-element": "^2.2.0",
    "lit-html": "^1.1.1"
  }
