{
  "name": "optimal-img",
  "version": "1.0.0",
  "description": "An image component that will automatically pull down smaller images based on screen size.",
  "main": "index.html",
  "scripts": {
    "build": "buildkit-legacy-scripts build",
    "aem-import": "buildkit-legacy-scripts aem-import",
    "start": "owc-dev-server --port 3000",
