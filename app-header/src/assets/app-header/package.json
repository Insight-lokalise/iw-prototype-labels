{
  "name": "app-header",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "buildkit-legacy-scripts start",
    "build": "buildkit-legacy-scripts build",
    "aem-import": "buildkit-legacy-scripts aem-import",
    "import-labels": "buildkit-legacy-scripts labels app-header",
    "test": "buildkit-asserter --coverage",
    "test:watch": "buildkit-asserter --watch",
    "lint": "buildkit-legacy-scripts lint",
    "prettify": "buildkit-legacy-scripts prettify",
    "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions"
  },
  "resolutions": {
    "faye": "1.1.3"
  },
  "jestSonar": {
    "sonar56x": true,
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "dependencies": {
    "@insight/toolkit-css-core": "^2.2.17",
    "@insight/toolkit-css-ui": "^2.2.126",
    "@insight/toolkit-react": "^2.2.126",
    "@insight/toolkit-utils": "^2.2.95",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "date-fns": "2.16.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-html-parser": "^2.0.2",
    "string-hash": "^1.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "axios-mock-adapter": "^1.19.0",
    "buildkit-asserter": "^1.0.36",
    "buildkit-legacy-scripts": "^5.0.0-Beta.8",
    "cross-env": "^7.0.3",
    "prop-types": "^15.8.1"
  }
}
