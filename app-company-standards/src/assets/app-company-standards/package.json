    "classnames": "^2.2.6",
    "draft-js": "^0.11.7",
    "react": "^16.8.1",
    "react-beautiful-dnd": "^10.0.4",
    "react-dom": "^16.8.1",
    "react-html-parser": "^2.0.2",
