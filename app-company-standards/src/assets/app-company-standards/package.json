    "buildkit-legacy-scripts": "^5.0.0-Beta.8",
    "@testing-library/react": "^12.0.0",
    "buildkit-asserter": "^1.0.36",
    "cross-env": "^5.2.0",
    "prop-types": "^15.6.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "why-did-you-update": "^1.0.6"
