{
  "name": "app-product-list",
  "description": "Displays a list of products as a carousel or grid.",
  "version": "1.0.0",
  "dependencies": {
    "@insight/toolkit-css-core": "^2.2.17",
    "@insight/toolkit-css-ui": "^2.2.45",
    "@insight/toolkit-react": "^2.2.54",
    "@insight/toolkit-utils": "^2.2.50",
    "axios": "^0.27.2",
    "classnames": "^2.2.6",
    "jsonpath-plus": "^5.0.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "resolutions": {
    "faye": "1.1.3"
  },
  "devDependencies": {
    "buildkit-legacy-scripts": "^5.0.0-Beta.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "buildkit-asserter": "^1.0.36",
    "prop-types": "^15.7.2",
    "why-did-you-update": "^1.0.6"
  },
  "aem": {
    "categories": "insight-product-list-react-app"
  },
  "scripts": {
    "start": "buildkit-legacy-scripts start",
    "build": "buildkit-legacy-scripts build",
    "aem-import": "buildkit-legacy-scripts aem-import",
    "import-labels": "buildkit-legacy-scripts labels app-product-list",
    "test": "buildkit-asserter --coverage",
    "test:watch": "buildkit-asserter --watch",
    "lint": "buildkit-legacy-scripts lint",
    "prettify": "buildkit-legacy-scripts prettify",
    "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions"
